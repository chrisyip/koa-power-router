{
  "name": "koa-power-router",
  "version": "0.0.7",
  "description": "Yet another router middleware for Koa, but better.",
  "main": "index.js",
  "scripts": {
    "mocha": "node --harmony node_modules/mocha/bin/_mocha -r test/env/server.js --reporter nyan --bail --check-leaks test/*.js",
    "mocha:cov": "node --harmony node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -r test/env/server.js --reporter spec --check-leaks test/*.js",
    "test": "gulp lint && npm run mocha",
    "test-travis": "gulp lint && npm run mocha:cov"
  },
  "author": "Chris Yip <i@chrisyip.im>",
  "license": "MIT",
  "homepage": "https://github.com/chrisyip/koa-power-router",
  "bugs": {
    "url": "https://github.com/chrisyip/koa-power-router/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisyip/koa-power-router.git"
  },
  "dependencies": {
    "async": "^0.9.0",
    "lodash": "^2.4.1",
    "path-to-regexp": "^1.0.1",
    "yieldr": "^0.1.0"
  },
  "devDependencies": {
    "gulp": "~3.8.8",
    "gulp-jshint": "~1.8.4",
    "gulp-util": "~3.0.1",
    "istanbul": "^0.3.5",
    "jshint-stylish": "~1.0.0",
    "koa": "^0.13.0",
    "mocha": "^2.1.0",
    "request": "^2.51.0"
  },
  "keywords": [
